{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "lua",
      "camelcase": "Lua",
      "title": "Lua",
      "scope": "source.lua",
      "file-types": [
        "lua"
      ],
      "injection-regex": "^lua$",
      "class-name": "TreeSitterLua"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "A lightweight, high-level, multi-paradigm programming language.",
    "authors": [
      {
        "name": "Panic Inc",
        "email": "logan@panic.com",
        "url": "https://panic.com/"
      }
    ],
    "links": {
      "repository": "https://github.com/panicinc/tree-sitter-lua"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}